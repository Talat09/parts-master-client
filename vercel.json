{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/"
    },
    {
      "src": "/api/auth",
      "dest": "/"
    },
    {
      "src": "/api/messages",
      "dest": "/"
    },
    {
      "src": "/api/users",
      "dest": "/"
    }
  ]
}
